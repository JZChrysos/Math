void oval(real x, real w, real h, pen c = black){
pair a,b,c,d,e,f;
a = (x-w,h);
b = (x,h+w);
c = (x+w,h);
d = (x+w,-h);
e = (x,-h-w);
f = (x-w,-h);
path p = a{up}..{right}b{right}..{down}c--d{down}..{left}e{left}..{up}f--a;
draw(p,c);
}
